{
  "name": "@laduke/zerotier-central-cli",
  "version": "0.0.8",
  "description": "Zerotier Central CLI",
  "keywords": [
    "zerotier",
    "oclif"
  ],
  "homepage": "https://github.com/laduke/zerotier-central-cli",
  "bugs": "https://github.com/laduke/zerotier-central-cli/issues",
  "repository": "laduke/zerotier-central-cli",
  "license": "MIT",
  "author": "Travis LaDuke @laduke",
  "main": "src/index.js",
  "bin": {
    "ztc": "./bin/run"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "scripts": {
    "build": "oclif-dev pack",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json",
    "test": "echo NO TESTS",
    "posttest": "standard",
    "version": "oclif-dev readme && git add README.md"
  },
  "eslintConfig": {
    "extends": [
      "standard"
    ]
  },
  "dependencies": {
    "@laduke/zerotier-central-client": "0.0.3",
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-autocomplete": "^0.1.5",
    "@oclif/plugin-help": "^2.2.3",
    "cli-ux": "^5.4.4",
    "conf": "^6.2.0",
    "is-cidr": "^3.1.0",
    "is-ip": "^3.1.0",
    "omit-empty": "^1.0.0",
    "tabtab": "^3.0.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-node": "^11.0.0",
    "globby": "^10.0.2",
    "standard": "*",
    "tape": "^5.0.0-next.4"
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "oclif": {
    "commands": "./src/commands",
    "topics": {
      "conf": {
        "description": "manage ztc cli config/preferences"
      },
      "network": {
        "description": "managed zerotier networks"
      },
      "member": {
        "description": "managed zerotier network members"
      }
    },
    "bin": "ztc",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
